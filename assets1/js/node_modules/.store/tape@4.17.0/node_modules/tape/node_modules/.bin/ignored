#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/dabtvis/assets1/js/node_modules/.store/dotignore@0.1.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/dabtvis/assets1/js/node_modules/.store/dotignore@0.1.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../dotignore@0.1.2/node_modules/dotignore/bin/ignored" "$@"
else
  exec node  "$basedir/../../../../../dotignore@0.1.2/node_modules/dotignore/bin/ignored" "$@"
fi
